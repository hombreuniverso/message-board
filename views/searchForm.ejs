<!--Search Form Template-->

<!DOCTYPE html>
<html lang="en">
  <!--Header Template-->
  <%- include('./partials/header')%>;
  <body>
    <!--Navbar Template-->
    <%- include('./partials/navbar', {links: links})%>
    <header>
      <h1><%=title%></h1>
    </header>
    <main>
      <form action="/search" method="get">
        <label for="search">Search:</label>
        <input
          type="text"
          name="search"
          placeholder="Search for username"
          required
        />
        <button type="submit">Search</button>
      </form>
    </main>
  </body>
</html>
