<!--Search Results-->

<!DOCTYPE html>
<html lang="en">
  <!--Header Template-->
  <%- include('./partials/header')%>;
  <body>
    <!--Navbar Template-->
    <%- include('./partials/navbar', {links: links})%>
    <header>
      <h1><%=title%></h1>
    </header>
    <main>
      <div class="message-container">
        <ul class="message-ul">
          <%if(results.length != 0){%>
          <%for(let i = 0; i < results.length; i++) {%>
          <li><%= results[i] %></li>
          <%}%>         
        <%}else{%>
        <li><%=message%></li>
        <%}%>
        </ul>
      </div>
    </main>
  </body>
</html>
